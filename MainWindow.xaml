<Window x:Class="Calculadora.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculadora"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="displaySaida" HorizontalAlignment="Left" Height="56" Margin="470,303,0,0" TextWrapping="Wrap" Text="LeroLero" VerticalAlignment="Top" Width="138" TextChanged="TextBox_TextChanged" TextAlignment="Center" RenderTransformOrigin="0.129,-0.5"/>
        <Button x:Name="calcular" Content="Enter" HorizontalAlignment="Left" Height="57" Margin="231,298,0,0" VerticalAlignment="Top" Width="130" Click="Button_Click" RenderTransformOrigin="-0.529,0.537"/>
        <TextBox x:Name="input2" HorizontalAlignment="Left" Margin="542,55,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="133" Height="56" TextChanged="TextBox_TextChanged_1"/>
        <TextBox x:Name="input1" HorizontalAlignment="Left" Height="56" Margin="139,55,0,0" TextWrapping="Wrap" Text="0&#xD;&#xA;" VerticalAlignment="Top" Width="135" TextChanged="input1_TextChanged"/>
        <ListBox HorizontalAlignment="Center" Height="43" Margin="0,62,0,0" VerticalAlignment="Top" Width="168" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Window>
